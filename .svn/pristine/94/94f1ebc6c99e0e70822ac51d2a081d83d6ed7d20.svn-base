    <servlet-class>com.vlm.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.vlm.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewCategoryList</servlet-name>
    <servlet-class>com.vlm.servlet.ViewCategoryList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewCategoryList</servlet-name>
    <url-pattern>/viewCategory</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewAlbumServlet</servlet-name>
    <servlet-class>com.vlm.servlet.ViewAlbumServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAlbumServlet</servlet-name>
    <url-pattern>/gobyid</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BorrowServlet</servlet-name>
    <servlet-class>com.vlm.servlet.BorrowServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BorrowServlet</servlet-name>
    <url-pattern>/borrow</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReturnServlet</servlet-name>
    <servlet-class>com.vlm.servlet.ReturnServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReturnServlet</servlet-name>
    <url-pattern>/return</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.vlm.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HistoryServlet</servlet-name>
    <servlet-class>com.vlm.servlet.HistoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HistoryServlet</servlet-name>
    <url-pattern>/history</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BackServlet</servlet-name>
    <servlet-class>com.vlm.servlet.BackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BackServlet</servlet-name>
    <url-pattern>/back</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>